<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="person" class="com.wen.spring.test.Person">
		<constructor-arg value="1992" index="0" name="id"></constructor-arg>
		<constructor-arg value="Wen" index="1" name="name"></constructor-arg>
		<property name="taxId" value="123"></property>
	</bean>
</beans>